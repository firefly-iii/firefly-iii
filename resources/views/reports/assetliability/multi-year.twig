{% extends './layout/default' %}

{% block breadcrumbs %}
    {{ Breadcrumbs.render(Route.getCurrentRoute.getName, accountIds, start, end) }}
{% endblock %}

{% block content %}
    <div class="row">
        <div class="col-lg-12">
            <div class="box">
                <div class="box-header with-border">
                    <h3 class="box-title"><i class="fa fa-area-chart"></i> Multi-Year Asset & Liability Trends</h3>
                </div>
                <div class="box-body">
                    <p>Historical account balance trends from {{ start.format('Y') }} to {{ end.format('Y') }}.</p>
                </div>
            </div>
        </div>
    </div>
{% endblock %}

